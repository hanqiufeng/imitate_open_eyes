.circle_container {
  width: 100%;
  background-color: #fafafa;
}

.list_items {
  display: flex;
  flex-direction: column;
  width: 95%;
  margin: 20rpx auto;
  background-color: #fff;
}

.list_item {
  border: 1px solid #f00;
  position: relative;
}

.item-introduce-img {
  border: 1px solid #000;
}

.item-introduce-img image {
  width: 100%;
  display: block;
}

.item-introduce-on {
  border: 3px solid #f00;
  width: 100%;
  align-items: center;
}

.item-introduce-on-title {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  text-align: center;
  font-size: 48rpx;
  color:#fff;
}

.introduce-img{
  width: 20rpx;
  height: 20rpx;
}

